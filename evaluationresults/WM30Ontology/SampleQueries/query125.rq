PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  ssn:  <http://purl.oclc.org/NET/ssnx/ssn#>

SELECT DISTINCT  ?Device_0
WHERE
  { ?Device_0  a                    ssn:Device
    NOT EXISTS
      { :SurvivalRange_instance23
                  ssn:hasSurvivalRange  ?Device_0
      }
  }
