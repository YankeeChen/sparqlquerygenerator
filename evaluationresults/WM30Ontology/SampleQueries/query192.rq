PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  WM30: <http://purl.oclc.org/NET/ssnx/meteo/WM30#>
PREFIX  ssn:  <http://purl.oclc.org/NET/ssnx/ssn#>

SELECT DISTINCT  ?Sensor_0
WHERE
  { ?Sensor_0  a                    ssn:Sensor
    { ?WM30_WindDirection_MeasurementCapability_0
                ssn:hasMeasurementCapability  ?Sensor_0
      { ?WM30_WindDirection_MeasurementCapability_0
                  a                     WM30:WM30_WindDirection_MeasurementCapability
        { :WindFeature_instance108
                    ssn:ofFeature  ?WM30_WindDirection_MeasurementCapability_0
        }
      }
    }
  }
