PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  WM30: <http://purl.oclc.org/NET/ssnx/meteo/WM30#>
PREFIX  ssn:  <http://purl.oclc.org/NET/ssnx/ssn#>

SELECT DISTINCT  ?WMS302_0
WHERE
  { ?WMS302_0  a                    WM30:WMS302
    OPTIONAL
      { ?WM30_WindSpeed_MeasurementCapability_WSgrt10ms_0
                  ssn:hasMeasurementCapability  ?WMS302_0
      }
  }
