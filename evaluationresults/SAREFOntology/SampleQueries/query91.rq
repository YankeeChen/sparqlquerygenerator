PREFIX  saref: <https://w3id.org/saref#>
PREFIX  ObjectDescription: <http://ece.neu.edu/ontologies/ObjectDescription3000.owl#>

SELECT DISTINCT  ?HVAC_0
WHERE
  { ?HVAC_0  a                     saref:HVAC
    { ?Time_0  saref:measuresProperty  ?HVAC_0
      { ?Time_0  a                     saref:Time }
    }
  }
