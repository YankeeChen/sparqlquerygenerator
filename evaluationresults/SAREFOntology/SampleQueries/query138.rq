PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?Actuator_0
WHERE
  { ?Actuator_0  a                  saref:Actuator
    { ?Power_0  saref:measuresProperty  ?Actuator_0
      { ?SmokeSensor_0
                  saref:isControlledByDevice  ?Power_0
        { ?SmokeSensor_0
                    a                     saref:SmokeSensor
        }
      }
    }
  }
