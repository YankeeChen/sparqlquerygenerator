PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?FunctionRelated_0
WHERE
  { ?FunctionRelated_0
              a                     saref:FunctionRelated
    OPTIONAL
      { ?Measurement_0
                  saref:makesMeasurement  ?FunctionRelated_0
        { ?Measurement_0
                    a                     saref:Measurement
        }
      }
  }
