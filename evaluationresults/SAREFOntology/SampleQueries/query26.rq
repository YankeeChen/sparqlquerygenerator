PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?DoorSwitch_0
WHERE
  { ?DoorSwitch_0
              a                     saref:DoorSwitch
    { ?LightSwitch_0
                saref:consistsOf  ?DoorSwitch_0
    }
  }
