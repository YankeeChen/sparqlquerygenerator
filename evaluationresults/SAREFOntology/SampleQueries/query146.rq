PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?Generator_0
WHERE
  { ?Generator_0  a                 saref:Generator
    { ?Measurement_0
                saref:makesMeasurement  ?Generator_0
      { ?Measurement_0
                  a                     saref:Measurement
      }
    }
  }
