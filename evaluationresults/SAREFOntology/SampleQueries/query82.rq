PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?Device_0
WHERE
  { ?Device_0  a                    saref:Device
    { ?Task_0  saref:accomplishes  ?Device_0
      { :MicroRenewable_instance100
                  saref:accomplishes  ?Task_0
      }
    }
  }
