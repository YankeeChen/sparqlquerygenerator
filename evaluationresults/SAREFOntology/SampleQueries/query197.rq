PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?Storage_0
WHERE
  { ?Storage_0  a                   saref:Storage
    { ?Storage_0  saref:hasFunction  ?Function_0
      { ?Function_0  a                  saref:Function
        EXISTS
          { ?GetCommand_0
                      saref:isCommandOf  ?Function_0
            { ?Function_0  saref:isCommandOf  ?GetCommand_0 }
          }
      }
    }
  }
