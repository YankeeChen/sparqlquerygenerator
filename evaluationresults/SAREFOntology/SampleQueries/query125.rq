PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?MicroRenewable_0
WHERE
  { ?MicroRenewable_0
              a                     saref:MicroRenewable
    { ?MicroRenewable_0
                saref:controlsProperty  ?Light_0
      { ?Light_0  a                     saref:Light }
    }
  }
