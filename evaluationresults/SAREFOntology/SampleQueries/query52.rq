PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?Network_0
WHERE
  { ?Network_0  a                   saref:Network
    { ?Network_0  saref:isOfferedBy  ?Service_0
      { ?Service_0  a                   saref:Service
        { ?MicroRenewable_0
                    saref:isOfferedBy  ?Service_0
        }
      }
    }
  }
