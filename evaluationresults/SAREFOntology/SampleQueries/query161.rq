PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?EnergyMeter_0
WHERE
  { ?EnergyMeter_0
              a                     saref:EnergyMeter
    { ?Energy_0  saref:controlsProperty  ?EnergyMeter_0
      { ?Energy_0  a                    saref:Energy }
    }
  }
