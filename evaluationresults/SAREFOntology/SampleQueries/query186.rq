PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?EnergyRelated_0
WHERE
  { ?EnergyRelated_0
              a                     saref:EnergyRelated
    { ?Var0  saref:hasTypicalConsumption  ?EnergyRelated_0
      { ?Var0  a                     saref:Power }
    }
  }
