PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?SmokeSensor_0
WHERE
  {   { ?SmokeSensor_0
                  a                     saref:SmokeSensor
      }
    UNION
      { :Occupancy_instance74
                  saref:controlsProperty  ?SmokeSensor_0
      }
  }
