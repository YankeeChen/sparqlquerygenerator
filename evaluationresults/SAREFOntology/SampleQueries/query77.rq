PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  saref: <https://w3id.org/saref#>

SELECT DISTINCT  ?Storage_0
WHERE
  { ?Storage_0  a                   saref:Storage
    { ?Storage_0  saref:consistsOf  ?DoorSwitch_0
      { ?DoorSwitch_0
                  a                     saref:DoorSwitch
      }
    }
  }
