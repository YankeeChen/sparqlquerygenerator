PREFIX  ContextAwareness_eDIANA: <http://www.owl-ontologies.com/ContextAwareness_eDIANA.owl#>
PREFIX  ObjectDescription: <http://ece.neu.edu/ontologies/ObjectDescription3000.owl#>

SELECT DISTINCT  ?Humidity_Sensor_0
WHERE
  { ?Humidity_Sensor_0
              a                     ContextAwareness_eDIANA:Humidity_Sensor
    { ObjectDescription:Humidity_Information_instance2
                ContextAwareness_eDIANA:related_comfort_properties  ?Humidity_Sensor_0
    }
  }
