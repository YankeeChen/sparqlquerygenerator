PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  ContextAwareness_eDIANA: <http://www.owl-ontologies.com/ContextAwareness_eDIANA.owl#>

SELECT DISTINCT  ?Threshold_Sensor_0
WHERE
  { ?Threshold_Sensor_0
              a                     ContextAwareness_eDIANA:Threshold_Sensor
    EXISTS
      { ?Var0  ContextAwareness_eDIANA:provided_services  ?Threshold_Sensor_0 }
  }
