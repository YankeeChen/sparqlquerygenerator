PREFIX  :     <http://ece.neu.edu/ontologies/DeviceDescription3000.owl#>
PREFIX  ContextAwareness_eDIANA: <http://www.owl-ontologies.com/ContextAwareness_eDIANA.owl#>

SELECT DISTINCT  ?Sound_Sensor_0
WHERE
  { ?Sound_Sensor_0
              a                     ContextAwareness_eDIANA:Sound_Sensor
    OPTIONAL
      { ?Var0  ContextAwareness_eDIANA:related_comfort_properties  ?Sound_Sensor_0 }
  }
